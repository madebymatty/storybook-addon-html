{
  "name": "@madebymatty/storybook-addon-html",
  "version": "1.0.0",
  "description": "A Storybook addon that extracts and displays compiled syntax-highlighted HTML",
  "keywords": [
    "addon",
    "storybook",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/madebymatty/storybook-addon-html.git"
  },
  "scripts": {
    "build": "yarn clean && babel -d ./out ./src",
    "build:watch": "babel --watch -d ./out ./src",
    "clean": "rm -rf ./out",
    "prepare": "yarn build",
    "prepublishOnly": "cp ../README.md ."
  },
  "author": "MadebyMatty",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/react": "^6.0.16",
    "babel-eslint": "^10.1.0",
    "react": "^16.13.1"
  },
  "dependencies": {
    "@babel/generator": "^7.11.0",
    "@babel/parser": "^7.11.2",
    "@storybook/addons": "^6.0.16",
    "@storybook/api": "^6.0.16",
    "@storybook/components": "^6.0.16",
    "prettier": "^2.0.5",
    "react-syntax-highlighter": "^12.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
